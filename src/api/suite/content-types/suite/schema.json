{
  "kind": "collectionType",
  "collectionName": "suites",
  "info": {
    "singularName": "suite",
    "pluralName": "suites",
    "displayName": "Suite",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "slider": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "addressURL": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "nativeName": {
      "type": "string"
    },
    "address": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "nativeAddress": {
      "type": "string"
    },
    "stay": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::stay.stay"
    },
    "faq": {
      "type": "component",
      "repeatable": true,
      "component": "stay.faq-item",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "WiFi": {
      "type": "component",
      "repeatable": true,
      "component": "stay.wi-fi-item"
    },
    "checkInOut": {
      "type": "customField",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "customField": "plugin::tinymce.tinymce"
    },
    "amenities": {
      "type": "customField",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "customField": "plugin::tinymce.tinymce"
    },
    "houseRules": {
      "type": "customField",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "customField": "plugin::tinymce.tinymce"
    }
  }
}
