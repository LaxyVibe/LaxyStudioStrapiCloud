{
    "filters": {
        "ownedBy": {
        	"slug": {
                "$eq": "beppu-story"
            }
        },
        "name": {
            "$eq": "family-room-1"
        }
    },
    "fields": [
		"name",
		"label",
		"headline",
		"address",
		"nativeAddress",
		"addressURL",
		"checkInOut",
		"amenities",
		"houseRules"
	],
    "populate": {
        "slider": {
            "fields": ["url"]
        },
        "faq": {
            "fields": ["question", "answer"]
        },
        "wifi": {
            "fields": ["network", "password"]
        },
        "ownedBy": {
            "fields": ["slug", "label", "greeting", "nativeLanguageCode"],
			"populate": {
				"avatar": {
					"fields": ["url"]
				}
			}
        }
    },
    "locale": "en"
}