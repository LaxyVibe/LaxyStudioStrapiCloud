{
    "filters": {
        "ownedBy": {
        	"slug": {
                "$eq": "beppu-story"
            }
        },
        "name": {
            "$eq": "family-room-1"
        }
    },
    "fields": [
		"name",
		"label",
		"headline",
		"address",
		"addressURL",
		"checkInOut",
		"amenities",
		"houseRules"
	],
    "populate": {
        "slider": {
            "fields": ["url"]
        },
        "faq": {
            "fields": ["question", "answer"]
        },
        "wifi": {
            "fields": ["network", "password"]
        },
        "ownedBy": {
            "fields": ["slug", "label", "greeting", "nativeLanguageCode"],
			"populate": {
				"avatar": {
					"fields": ["url"]
				},
				"pickedPOIs": {
					"fields": [
						"slug",
						"label",
						"address",
						"highlight",
						"externalURL",
						"type"
					],
					"populate":{
						"tag_labels": {
							"fields": ["name", "color"]
						},
						"coverPhoto": {
							"fields": ["url"]
						}
					}
				}
			}
        }
    },
    "locale": "en"
}