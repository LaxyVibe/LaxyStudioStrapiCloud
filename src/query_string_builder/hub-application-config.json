{
  "populate": {
    "universalConfig": {
      "populate": {
        "releasedLanguages": {
          "fields": ["label", "value"]
        }
      }
    },
    "header": {
      "fields": ["leftRoute","rightRoute"],
      "populate": {
        "leftIcon": {
          "fields": ["url"]
        },
        "rightIcon": {
          "fields": ["url"]
        }
      }
    },
    "pageLanding": {
      "fields": ["recommendationHeading"],
      "populate": {
        "naviagtion": {
          "fields": ["label", "route"],
          "populate": {
            "icon": {
              "fields": ["url"]
            }
          }
        }
      }
    },
    "pageLanguage": {
      "fields": ["heading"],
      "populate": {
        "applyButton": {
          "fields": ["label"]
        }
      }
    },
    "pageSearch": {
      "fields": ["searchInputPlaceholder", "defaultListHeading", "highlightedListHeading"],
      "populate": {
        "defaultList": {
          "fields": ["label", "value"]
        }
      }
    }
  }
}